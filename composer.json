{
    "name": "aSmithSummer/roadblock",
    "description": "Automated blocking of traffic based on security member, session and request infmoration that is also logged.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "ramsey/uuid": "^4.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "aSmithSummer\\Roadblock\\": "src/",
            "aSmithSummer\\Roadblock\\Tests\\": "tests/php/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
